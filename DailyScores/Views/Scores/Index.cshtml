@model IEnumerable<DailyScores.Models.EmailSubmission>

@{
    ViewBag.Title = "Index";
}

<h2>Scores</h2>
<ul>
    @foreach (var email in Model)
    {
        <li>@string.Format("(FROM:{0}{4}TO:{1}{4}SUBJECT:{2}{4}BODY:{3}", email.From, email.To, email.Subject, email.Body, Environment.NewLine)</li>
    }
</ul>
